abbr "gc"="git commit"
abbr "gcm"="git commit -m '%'"
abbr "gd"="git diff"
abbr "gs"="git status"
abbr "gss"="git status --short"
abbr "gca"="git commit --amend"
abbr "gaa"="git add ."
abbr "l"="ls -l"
abbr "la"="ls -a"
abbr "chezmoiaa"="chezmoi re-add $(chezmoi status | cut -d " " -f2)"
